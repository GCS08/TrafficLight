<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Style x:Key="MyButtonStyle" TargetType="Button">
        <Setter Property="TextColor" Value="Black" />
        <Setter Property="CornerRadius" Value="25" />
    </Style>
    <Style
        x:Key="MySpButtonStyle"
        BasedOn="{x:StaticResource MyButtonStyle}"
        TargetType="Button">
        <Setter Property="BorderWidth" Value="10" />
        <Setter Property="BorderColor" Value="Cyan" />
    </Style>
    <Style TargetType="Frame">
        <Setter Property="CornerRadius" Value="55" />
        <Setter Property="BackgroundColor" Value="Black" />
        <Setter Property="HeightRequest" Value="110" />
        <Setter Property="WidthRequest" Value="110" />
        <Setter Property="Margin" Value="10" />
    </Style>
    <Style TargetType="BoxView">
        <Setter Property="CornerRadius" Value="50" />
        <Setter Property="WidthRequest" Value="100" />
        <Setter Property="HeightRequest" Value="100" />
    </Style>
</ResourceDictionary>